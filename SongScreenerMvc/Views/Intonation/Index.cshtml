@model IEnumerable<SongScreenerMvc.Models.Intonation>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            IntonationName
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IntonationName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IntonationID }) |
            @Html.ActionLink("Details", "Details", new { id=item.IntonationID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IntonationID })
        </td>
    </tr>
}

</table>
