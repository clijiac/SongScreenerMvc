@model SongScreenerMvc.Models.Entity.Artist

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Artist</legend>

    <div class="display-label">ArtistName</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ArtistName)
    </div>

    <div class="display-label">Gender</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Gender.GenderName)
    </div>

    <div class="display-label">Hometown</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Hometown.HometownName)
    </div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.ArtistID }) |
    @Html.ActionLink("Back to List", "Index")
</p>
